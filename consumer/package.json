{
  "name": "consumer",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.27.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.16.0",
    "react-router-dom": "6.16.0",
    "react-scripts": "5.0.1",
    "spectre.css": "0.5.9",
    "web-vitals": "3.4.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "12.1.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "nock": "13.3.3",
    "rimraf": "5.0.4",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "pretest:pact": "rimraf pacts/*.json",
    "test:pact": "CI=true react-scripts test --testTimeout=30000 pact.spec.js",
    "test:unit": "CI=true react-scripts test api.spec.js",
    "pact:publish:local": "node --env-file=.env.local ./node_modules/@pact-foundation/pact-core/bin/pact-broker publish ./pacts --consumer-app-version=\"1.0.2\" --auto-detect-version-properties",
    "pact:publish": "node --env-file=.env ./node_modules/@pact-foundation/pact-core/bin/pact-broker publish ./pacts --consumer-app-version=\"1.0.2\" --auto-detect-version-properties"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
